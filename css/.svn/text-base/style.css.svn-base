@charset "utf-8";
/* CSS Document */
*{
margin:0px;
padding:0px;
}
#wrapper{
width:960px;
margin-left:auto;
margin-right:auto;
}

body{
background-image:url('../images/body_bg.jpg');
background-repeat:repeat-x;
width:100%;
min-height:800px;
color:#818181;
background-color:#fff;
}

.form_para{
font-size:11px;
font-family:Tahoma;
color:#818181;
margin-left:50px;
padding-left:5px;
}

.form_para2{
color:#C00;
font-size:11px;
font-family:Tahoma;
margin-bottom:70px;
text-align:center;
}

#logo{
margin-top:100px;
width:523px;
text-align:right;
margin-left:auto;
margin-right:auto;
}



#login_form{
width:523px;
height:348px;
margin-left:auto;
margin-right:auto;
background-image:url('../images/form_bg.png');
background-repeat:no-repeat;
margin-top:0px;

}

.form_style td{
	height:30px;
}

.table_style {
	padding:5px;
	margin-left:100px;
	font-family:Tahoma, Geneva, sans-serif;
	font-size:12px;
	color:#686868;
	
}
.table_style td{
	padding:5px;
	
}
.table_style2 {
	padding:0px;
	margin-left:100px;
	margin-top:-15px;
}

input{
	border:1px solid #c6c6c6;
	height:25px;
}

.submit_btn{
background-image:url('../images/login_btn.png');
width:80px;
height:26px;
background-repeat:no-repeat;
border:none;
background-color:transparent;
margin-left:110px;

}

.submit_btn:hover{
background-image:url('../images/login_btn_hover.png');
cursor:pointer;
}